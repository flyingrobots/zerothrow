{
  "name": "@zerothrow/graph-ext",
  "version": "0.1.0",
  "description": "Extended features for @zerothrow/graph - semantic edges, builders, state machines",
  "author": "ZeroThrow Contributors",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@zerothrow/graph": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.16.16",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {},
  "keywords": [
    "graph",
    "state-machine",
    "fsm",
    "semantic-edges",
    "graph-traversal",
    "workflow",
    "typescript"
  ]
}