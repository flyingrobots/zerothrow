{
  "name": "@zerothrow/jest",
  "version": "1.1.0",
  "description": "Jest matchers for ZeroThrow Result types",
  "author": "J. Kirby Ross <james@flyingrobots.dev> (https://github.com/flyingrobots)",
  "license": "MIT",
  "homepage": "https://github.com/zerothrow/zerothrow#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerothrow/zerothrow.git",
    "directory": "packages/jest"
  },
  "bugs": {
    "url": "https://github.com/zerothrow/zerothrow/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src test --max-warnings 0"
  },
  "peerDependencies": {
    "@jest/globals": ">=27.0.0",
    "@zerothrow/core": ">=0.1.0",
    "@zerothrow/expect": ">=0.1.0",
    "jest": ">=27.0.0"
  },
  "peerDependenciesMeta": {
    "@jest/globals": {
      "optional": true
    }
  },
  "devDependencies": {
    "@zerothrow/expect": "workspace:*"
  },
  "keywords": [
    "zerothrow",
    "jest",
    "matchers",
    "testing",
    "result",
    "error-handling",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "platform": "node",
  "sideEffects": false
}
